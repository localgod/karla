# Local development environment configuration
# Copy this file to .env and update with your local paths

# Path to ImageMagick binaries (include trailing slash)
# Linux/macOS: /usr/bin/ or /usr/local/bin/ or /opt/homebrew/bin/
# Windows: C:/Program Files/ImageMagick/ or C:/dev/imagemagick/
IMAGEMAGICK_PATH=/usr/bin/

# PHP binary path (optional, used for some development tasks)
PHP_PATH=/usr/bin/php

# Cache directory for test files (optional)
CACHE_DIR=./tests/unit/_cache
